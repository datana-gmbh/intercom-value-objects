{
    "name": "datana-gmbh/intercom-value-objects",
    "type": "library",
    "description": "A set of value objects which can be used with intercom/intercom-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Stark",
            "email": "oskarstark@googlemail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "intercom/intercom-php": "^4.4",
        "nyholm/psr7": "^1.8",
        "oskarstark/trimmed-non-empty-string": "^1.0",
        "php-http/message-factory": "^1.1",
        "symfony/http-client": "^6.0||^7.0",
        "symfony/http-foundation": "^5.4||^6.0||^7.0",
        "thecodingmachine/safe": "^2.0",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "ergebnis/test-util": "^1.4",
        "phpunit/phpunit": "^9.0",
        "symfony/string": "^5.1 || ^6.0 || ^7.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "Datana\\Intercom\\Value\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datana\\Intercom\\Value\\Tests\\": "tests/"
        }
    }
}
