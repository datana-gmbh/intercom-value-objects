{
    "name": "gansel-rechtsanwaelte/intercom-value-objects",
    "type": "library",
    "description": "A set of value objects which can be used with intercom/intercom-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Stark",
            "email": "oskarstark@googlemail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "intercom/intercom-php": "^4.4",
        "php-http/guzzle7-adapter": "^0.1.1",
        "thecodingmachine/safe": "^1.3",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "ergebnis/test-util": "^1.4",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Gansel\\Intercom\\Value\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gansel\\Intercom\\Value\\Tests\\": "tests/"
        }
    }
}
